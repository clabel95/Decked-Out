  },
  "homepage": "https://decked--out.herokuapp.com/",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "jest-dom": "^4.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "apollo-server-express": "^3.6.4",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "express": "^4.17.3",
    "gh-pages": "^3.2.3",
    "graphql": "^16.3.0",
    "jwt-decode": "^3.1.2",
    "materialize-css": "^1.0.0",
    "mongoose": "^6.2.6",
